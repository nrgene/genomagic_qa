#!/usr/bin/env python
import numpy

def selectRandom(data,pos,k):
    n=len(data)
    x=numpy.ceil(n*numpy.random.sample(k)).astype(int)
    return x

def myMin(v):
    n=len(v)
    min_val=v[0]+1
    min_ind=0
    for i in range(n):
        if v[i]<min_val:
            min_val=v[i]
            min_ind=i
    return [min_val,min_ind]

